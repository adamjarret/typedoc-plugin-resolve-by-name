{
  "name": "typedoc-plugin-resolve-by-name",
  "version": "0.1.0",
  "description": "Fall back to name-based type resolution",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "ncu": "ncu",
    "fix": "yarn pretty && yarn lint && yarn spell",
    "pretty": "prettier './**/*.{ts,js,json,md}' --write --ignore-path .eslintignore",
    "lint": "eslint --ext .ts --ext .js .",
    "spell": "cspell './**/*' --config .vscode/cSpell.json",
    "clean": "rimraf dist '**/tsconfig.tsbuildinfo'",
    "build": "tsc"
  },
  "keywords": [
    "typedocplugin",
    "typedoc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adamjarret/typedoc-plugin-resolve-by-name"
  },
  "author": "Adam Jarret <adam@atj.me> (https://atj.me)",
  "license": "MIT",
  "peerDependencies": {
    "typedoc": "^0.17.7"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "cspell": "^4.0.63",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "npm-check-updates": "^7.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.5"
  }
}
